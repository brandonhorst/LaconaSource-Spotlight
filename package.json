{
  "name": "lacona-osx",
  "version": "1.0.4",
  "description": "Lacona Extensions for OSX, tied into the global-context hosted API",
  "lacona": {
    "config": "config.json"
  },
  "main": "lib-node/index.js",
  "jsnext:main": "lib-es/index.js",
  "browser": "lib-browser/index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=node mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "npm run build:es & npm run build:browser & npm run build:node & wait",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir lib-es",
    "build:browser": "cross-env BABEL_ENV=browser babel src --out-dir lib-browser",
    "build:node": "cross-env BABEL_ENV=node babel src --out-dir lib-node",
    "clean": "rimraf lib-browser lib-node lib-es",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lacona/lacona-osx.git"
  },
  "keywords": [
    "Lacona",
    "Source",
    "spotlight",
    "mdfind",
    "app",
    "file",
    "osx",
    "search"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacona/lacona-osx/issues"
  },
  "homepage": "https://github.com/lacona/lacona-osx",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "elliptical": "^2.0.0",
    "lacona-api": "^1.4.0",
    "lacona-phrases": "^1.0.1",
    "lodash": "^4.16.4",
    "rxjs": "^5.0.0-rc.1"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-async-to-generator",
      "transform-function-bind",
      "syntax-async-functions"
    ],
    "env": {
      "browser": {
        "presets": [
          "es2015"
        ],
        "plugins": [
          "transform-react-jsx",
          [
            "transform-runtime",
            {
              "polyfill": false,
              "regenerator": true
            }
          ]
        ]
      },
      "es": {
        "presets": [
          "es2015-native-modules"
        ],
        "plugins": [
          "transform-react-jsx",
          [
            "transform-runtime",
            {
              "polyfill": false,
              "regenerator": true
            }
          ]
        ]
      },
      "node": {
        "presets": [
          "es2015-node"
        ]
      }
    }
  }
}
