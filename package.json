{
  "name": "lacona-osx",
  "version": "0.0.1",
  "description": "Lacona Extensions for OSX, tied into the global-context hosted API",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p lib; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lacona/lacona-osx.git"
  },
  "keywords": [
    "Lacona",
    "Source",
    "spotlight",
    "mdfind",
    "app",
    "file",
    "osx",
    "search"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacona/lacona-osx/issues"
  },
  "homepage": "https://github.com/lacona/lacona-osx",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc9",
    "elliptical-datetime": "^1.0.0-rc1",
    "elliptical-email": "^1.0.0-rc2",
    "elliptical-phone": "^1.0.0-rc2",
    "elliptical-url": "^1.0.0-rc2",
    "lacona-api": "0.1.0",
    "lacona-command": "^0.2.0",
    "lacona-phrases": "0.0.2",
    "lodash": "^4.9.0",
    "rxjs": "^5.0.0-beta.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
